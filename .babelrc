{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "esmodules": true
        },
        "modules": "auto"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-proposal-private-methods",
      {
        "loose": true
      }
    ]
  ]
}