{
    "name": "$name",
    "short_name": "$shortName",
    "version": "$version",
    "version_name": "$semanticVersion",
    "description": "$description",
    "manifest_version": 2,
    "author": "$author",
    "developer": "$developer",
    "homepage_url": "$homepageURL",
    "content_scripts": [{
        "matches": [
            "*://aniwatch.me/*"
        ],
        "js": [
            "javascript/app.min.js"
        ],
        "run_at": "document_start"
    }]
}